
UPDATE datawarehouse.patient
SET citizenship = 
(CASE citizenship
WHEN 'AFG' THEN 'AFGHANISTAN'
WHEN 'ALB' THEN 'ALBANIA'
WHEN 'DZA' THEN 'ALGERIA'
WHEN 'ASM' THEN 'AMERICAN_SAMOA'
WHEN 'AND' THEN 'ANDORRA'
WHEN 'AGO' THEN 'ANGOLA'
WHEN 'AIA' THEN 'ANGUILLA'
