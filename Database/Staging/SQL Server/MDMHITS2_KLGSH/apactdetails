CREATE TABLE "apactdetails"( 
	"id" varchar(36) NOT NULL,
	"apactheaderid" varchar(36) NOT NULL,
	"appoperationid" varchar(36),
	"source" varchar(4),
	"trantype" varchar(4),
	"auditno" int,
	"line" int,
	"entrydate" timestamp,
	"documentno" varchar(36),
	"categoryid" varchar(36),
	"reference" varchar(36),
	"departmentid" varchar(36),
	"amount" numeric(21, 8),
	"stat" varchar(2),
	"patientid" varchar(36),
	"episodeid" varchar(36),
	"billno" int,
	"paymodeid" varchar(36),
	"deliveryorderheaderid" varchar(36),
	"deliveryorderdetailid" varchar(36),
	"companyid" varchar(36) NOT NULL,
	"updateuserid" varchar(36) NOT NULL,
	"updatedate" timestamp NOT NULL,
	"noteexist" boolean,
	"taxdetailid" varchar(36),
	"status" varchar(4),
	"taxamount" numeric(21, 8),
	"allocauditno" int,
	"alloctnauditno" int)
DISTRIBUTED BY (id);

